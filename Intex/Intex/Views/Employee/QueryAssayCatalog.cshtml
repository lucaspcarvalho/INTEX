@{
    ViewBag.Title = "QueryAssayCatalog";
}

<h2>Search Assays </h2>
<form class="form-horizontal">
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">Assay ID</label>
        <div class="col-sm-10">
            <input type="AssayID" class="form-control" id="AssayID" placeholder="Assay ID">
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-2 control-label">Name</label>
        <div class="col-sm-10">
            <input type="Name" class="form-control" id="Name" placeholder="Name">
        </div>
    </div>


    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <a href="/Employee/UpdateAssayCatalog"><button type="button" class="btn btn-default">Search</button></a>
        </div>
    </div>
</form>