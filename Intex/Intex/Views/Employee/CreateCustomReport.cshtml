
@{
    ViewBag.Title = "CreateCustomReport";
}

<h2>Create a Custom Report</h2>
<form class="form-horizontal">
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">Your Custom Search</label>
        <div class="col-sm-10">
            <input type="CustomerID" value="7392" class="form-control" id="CustomerID" placeholder="Pick from WorkOrderID, CompoundSampleID, AssayID or whatever you need">
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <a href="/Employee/CustomReportReview"><button type="button" class="btn btn-default">Search</button></a>
        </div>
    </div>
</form>